    "next": "9.3.2",
    "node-sass": "4.13.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "tailwindcss": "1.2.0",
    "unstated-next": "1.1.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "2.0.6",
    "@next/bundle-analyzer": "9.2.2",
    "@types/node": "13.7.4",
    "@types/react": "16.9.22",
    "@types/react-dom": "16.9.5",
    "autoprefixer": "9.7.4",
    "cross-env": "7.0.0",
    "cssnano": "4.1.10",
